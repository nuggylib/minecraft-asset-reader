    "inquirer": "^8.0.0",
    "lerna": "^4.0.0",
    "lodash": "^4.17.21",
    "mkdirp": "^1.0.4",
    "ngrok": "^3.4.1",
    "node-cache": "^5.1.2",
    "node-emoji": "^1.10.0",
    "node-fetch": "^2.6.1",
    "open": "^8.0.2",
    "react": "^16.8.2",
    "redis": "^3.0.2",
    "unzipper": "^0.10.11",
