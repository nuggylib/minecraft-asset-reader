{
  "name": "minecraft-asset-reader",
  "version": "0.1.0-alpha",
  "description": "Read in textures from a minecraft texture pack",
  "main": "packages/cli/src/main.tsx",
  "bin": {
    "minecraft-asset-reader": "bin/script.sh"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "cli",
    "minecraft-asset-reader"
  ],
  "scripts": {
    "clean": "rm -rf out/ dist/",
    "reset": "yarn clean && npm run build",
    "lint:commit": "prettier-eslint \"{src/**/*,test/**/*,packages/**/*}.{ts,tsx}\" --eslint-config-path=.eslintrc.yml --write",
    "prepare": "husky install",
    "reset-db": "rm -rf /tmp/minecraft-asset-reader",
    "build-app": "yarn build:cli && yarn build:client",
    "run-app": "node out/cli/src/main.js",
    "start:cli": "lerna exec --scope 'minecraft-asset-reader' yarn start",
    "build:cli": "lerna exec --scope 'minecraft-asset-reader' yarn build && cd out/ && rm -rf client/ && cd .. && cp packages/cli/package.json out/cli && cd out/cli && npm install",
    "start:client": "lerna exec --scope 'minecraft-asset-reader-webapp' yarn start",
    "build:client": "lerna exec --scope 'minecraft-asset-reader-webapp' yarn build && cp -r packages/client/build out && rm -rf packages/client/build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nuggylib/minecraft-asset-reader.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nuggylib/minecraft-asset-reader/issues"
  },
  "homepage": "https://github.com/nuggylib/minecraft-asset-reader#readme",
  "devDependencies": {
    "@babel/preset-env": "^7.13.15",
    "@babel/preset-react": "^7.13.13",
    "@emotion/eslint-plugin": "^11.2.0",
    "@testing-library/react-hooks": "^5.1.1",
    "@types/cors": "^2.8.10",
    "@types/express-paginate": "^1.0.0",
    "@types/ink-big-text": "^1.2.0",
    "@types/ink-divider": "^2.0.1",
    "@types/ink-gradient": "^2.0.1",
    "@types/inquirer": "^7.3.1",
    "@types/jest": "^26.0.22",
    "@types/lodash": "^4.14.168",
    "@types/mkdirp": "^1.0.1",
    "@types/node": "^14.14.33",
    "@types/node-emoji": "^1.8.1",
    "@types/node-fetch": "^2.5.8",
    "@types/react": "^17.0.3",
    "@types/react-test-renderer": "^17.0.1",
    "@types/redis": "^2.8.28",
    "@typescript-eslint/eslint-plugin": "^4.21.0",
    "@typescript-eslint/parser": "^4.21.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.23.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-plugin-cypress": "^2.11.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.23.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^6.0.0",
    "lerna": "^4.0.0",
    "lint-staged": ">=10",
    "prettier": "^2.2.1",
    "prettier-eslint-cli": "^5.0.1",
    "react-test-renderer": "^16.8.2",
    "ts-jest": "^26.5.4",
    "ts-node": "^9.1.1",
    "typedoc": "^0.20.30",
    "typescript": "^4.2.3"
  },
  "dependencies": {
    "dotenv": "^9.0.2",
    "esm": "^3.2.25",
    "minecraft-asset-reader": "file:packages/cli",
    "minecraft-asset-reader-webapp": "file:packages/client"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx,json,md,yml,yaml}": [
      "yarn lint:commit"
    ]
  }
}
